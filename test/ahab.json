{
    "ahab": "0.1",
    "command": "dockerd",
    "image": "docker:19.03-dind",
    "init": [
        "apk add go"
    ],
    "options": [
        "--privileged"
    ],
    "permissions": {
        "cmdSet": "busybox",
        "groups": [
            "!docker"
        ],
        "restartAfterSetup": true
    },
    "volumes": [
        "../:/home/ahab/src",
        "./mnt:/mnt",
        "./config.json:/home/ahab/.config/ahab/config.json"
    ],
    "workdir": "/home/ahab/src"
}
